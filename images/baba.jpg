<script type="text/javascript">

    var inappropriateContent = false;

            var _rate = 9.0;
        var _angle = 360 * _rate / 10;
        document.getElementById('rating-graph-arc').setAttribute('d', describeArc(18, 18, 16.75, 0, _angle));
    
    var movieId = "689";
    var isTvSeries = false;
    var embedVideoId = "6762773";
    var PLAYER_HEIGHT = 360;
    var PLAYER_WIDTH = 615;

    

        function polarToCartesian(centerX, centerY, radius, angleInDegrees) {
            var angleInRadians = (angleInDegrees-90) * Math.PI / 180.0;
            return {
                x: centerX + (radius * Math.cos(angleInRadians)),
                y: centerY + (radius * Math.sin(angleInRadians))
            };
        }

        function describeArc(x, y, radius, startAngle, endAngle) {
            var start = polarToCartesian(x, y, radius, endAngle);
            var end = polarToCartesian(x, y, radius, startAngle);
            var largeArcFlag = endAngle - startAngle <= 180 ? '0' : '1';
            var d = [
                'M', start.x, start.y,
                'A', radius, radius, 0, largeArcFlag, 0, end.x, end.y
            ].join(' ');
            return d;
        }

        function videoEnd(){
          var currentSlide = $('.trailer-list').slick('slickCurrentSlide');
          if(currentSlide + 1 == $('.trailer-item').length){
            currentSlide = 0;
          }
          else{
            currentSlide += 1;
          }

          $($('.trailer-item')[currentSlide]).trigger('click');
        }

        window.addEventListener('load', function () {

            $('.trailer-list').slick({
                infinite: false,
                slidesToShow: 5,
                slidesToScroll: 1,
                prevArrow: $('.trailer-prev'),
                nextArrow: $('.trailer-next')
            });

            $('.trailer-item').click(function(){
              $('.trailer-list').slick('slickGoTo', this.dataset.index, false);
              $('.trailer-item.active').removeClass('active');
              $(this).addClass('active');
            });

            $('.stars').rating({
                callback: function (value, link) {
                    $.post('/ajax/movie/rateMovie/' + movieId + '/' + value, {}, function (result) {
                        if (result) {
                            alert('Puanınız ' + (value / 4).toFixed(2) +  ' olarak güncellenmiştir!');
                            $('.rate-movie').addClass('hide');
                        }
                    });
                }
            });

